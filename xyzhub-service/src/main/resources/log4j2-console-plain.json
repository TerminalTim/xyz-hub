{
  "configuration": {
    "status": "info",

    "name": "Default Log Config",
    "packages": "com.here.xyz",

    "ThresholdFilter": {
      "level": "info"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d %-5p %c{1} %.-4096msg%throwable%n%xEx{none}"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}
